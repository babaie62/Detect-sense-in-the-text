   "cell_type": "code",
   "execution_count": 53,
   "metadata": {},
   "outputs": [],
   "source": [
    "SVC = LinearSVC(C=2.5, penalty=\"l2\", dual=False ,tol=0.0025).fit(train_data_features_BOW, train[\"sentiment\"] )\n",
    "result_SVC = SVC.predict(dev_data_features_BOW)\n",
    "D_SVC = precision_recall_fscore_support(lable_dev, result_SVC, average='macro')\n",
    "print('SVM Classifier_BOW :', '  **P=',D_SVC [0], '   **R=',D_SVC [1],'   **F=', D_SVC [2])"
