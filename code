   "source": [
    "print (\"Creating the Tfidf ...\\n\")\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "vectorizer = TfidfVectorizer(analyzer = \"word\",max_features = 5000,max_df=0.01,ngram_range=(1, 3)) \n",
    "train_data_features=vectorizer.fit_transform(clean_train_reviews)\n",
    "dev_data_features=vectorizer.transform(clean_dev_reviews)\n",
    "lable_dev = np.array(dev['sentiment'])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# رویکرد یادگیری ماشین-یادگیری نظارت شده "
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# 1- RandomForestClassifier"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "metadata": {},
   "outputs": [],
   "source": [
    "forest = RandomForestClassifier(n_estimators = 30) \n",
    "forest = forest.fit( train_data_features_BOW, train[\"sentiment\"] )\n",
    "# پیش بینی\n",
    "result_forest = forest.predict(dev_data_features_BOW)\n",
    "\n",
    "# سنجش میزان دقت \n",
    "D_forest = precision_recall_fscore_support(lable_dev, result_forest, average='macro')\n",
    "print('RandomForest Classifier_BOW :', '  **P=',D_forest[0], '   **R=',D_forest[1],'   **F=', D_forest[2])"
   ]
