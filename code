   "outputs": [],
   "source": [
    "SVC = LinearSVC(C=2.5, penalty=\"l2\", dual=False ,tol=0.0025).fit(train_data_features, train[\"sentiment\"] )\n",
    "result_SVC = SVC.predict(dev_data_features)\n",
    "D_SVC = precision_recall_fscore_support(lable_dev, result_SVC, average='macro')\n",
    "print('SVM Classifier_TFIDF :', '  **P=',D_SVC [0], '   **R=',D_SVC [1],'   **F=', D_SVC [2])"
   ]
  },
