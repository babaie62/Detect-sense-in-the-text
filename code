  {
   "cell_type": "code",
   "execution_count": 50,
   "metadata": {},
   "outputs": [],
   "source": [
    "forest = RandomForestClassifier(n_estimators = 50)   \n",
    "forest = forest.fit( train_data_features, train[\"sentiment\"] )\n",
    "# پیش بینی\n",
    "result_forest = forest.predict(dev_data_features)\n",
    "\n",
    "# سنجش میزان دقت \n",
    "D_forest = precision_recall_fscore_support(lable_dev, result_forest, average='macro')\n",
    "print('RandomForest Classifier_TFIDF :', '  **P=',D_forest[0], '   **R=',D_forest[1],'   **F=', D_forest[2])"
